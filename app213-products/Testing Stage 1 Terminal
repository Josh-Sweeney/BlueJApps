product1.decreaseQuantity(1);
product1.print();
 ID 101: Samsung Galaxy S7 Edge stock level: 0
product1.increaseQuantity(2);
product1.print();
 ID 101: Samsung Galaxy S7 Edge stock level: 2
product1.decreaseQuantity(1);
product1.print();
 ID 101: Samsung Galaxy S7 Edge stock level: 1
Product product2 = new Product(102, "Samsung Galaxy S7");
StockList stockList = new StockList();
stockList.add(product1);
stockList.add(product2);
stockList.print();

